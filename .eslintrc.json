{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": ["eslint:recommended", "plugin:react/recommended", "prettier"],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "indent": 0,
        "linebreak-style": 0,
        "quotes": [
            "error",
            "double"
        ],
        "semi": 0,
        "no-console": "warn",
        "react/prop-types": "warn",
        "no-unused-vars": "warn"
    }
}